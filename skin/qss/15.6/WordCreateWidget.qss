/**********QPushButton**********/
QPushButton#btn_cancel{
	qproperty-geometry:rect(250 2680 100 100);
	background:url(skin/picture/15.6/cross.png) 0 center no-repeat rgba(255,255,255,0);
}
QPushButton#btn_cancel:hover{
	background:url(skin/picture/15.6/cross_hover.png) 0 center no-repeat rgba(255,255,255,0);
}
QPushButton#btn_cancel:pressed{
	background:url(skin/picture/15.6/cross_pressed.png) 0 center no-repeat rgba(255,255,255,0);
}
QPushButton#btn_confirm{
	qproperty-geometry:rect(450 2680 100 100);
	background:url(skin/picture/15.6/hook.png) 0 center no-repeat rgba(255,255,255,0);
}
QPushButton#btn_confirm:hover{
	background:url(skin/picture/15.6/hook_hover.png) 0 center no-repeat rgba(255,255,255,0);
}
QPushButton#btn_confirm:pressed{
	background:url(skin/picture/15.6/hook_pressed.png) 0 center no-repeat rgba(255,255,255,0);
}
