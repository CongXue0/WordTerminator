/***********QCheckBox***********/
QCheckBox#checkBox_phrase{
	qproperty-geometry:rect(10 90 200 50);
	/*background:#005656;*/
	font-size:28px;
	color:#fc4343;
	spacing:5px;
}
QCheckBox#checkBox_phrase::indicator{
	width:50px;
	height:50px;
}
QCheckBox#checkBox_phrase::indicator::unchecked{
	image:url(skin/picture/15.6/check1_inactive.png);
}
QCheckBox#checkBox_phrase::indicator::checked{
	image:url(skin/picture/15.6/check1_active.png);
}

