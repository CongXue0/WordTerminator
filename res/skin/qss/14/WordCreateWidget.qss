/***********QCheckBox***********/
QCheckBox#checkBox_phrase{
	qproperty-geometry:rect(8 72 164 40);
	/*background:#005656;*/
	font-size:23px;
	color:#fc4343;
	spacing:4px;
}
QCheckBox#checkBox_phrase::indicator{
	width:40px;
	height:40px;
}
QCheckBox#checkBox_phrase::indicator::unchecked{
	image:url(skin/picture/14/check1_inactive.png);
}
QCheckBox#checkBox_phrase::indicator::checked{
	image:url(skin/picture/14/check1_active.png);
}

