/**********QPushButton**********/
QPushButton#btn_cancel{
	qproperty-geometry:rect(201 2144 80 80);
	background:url(skin/picture/14/cross.png) 0 center no-repeat rgba(255,255,255,0);
}
QPushButton#btn_cancel:hover{
	background:url(skin/picture/14/cross_hover.png) 0 center no-repeat rgba(255,255,255,0);
}
QPushButton#btn_cancel:pressed{
	background:url(skin/picture/14/cross_pressed.png) 0 center no-repeat rgba(255,255,255,0);
}
QPushButton#btn_confirm{
	qproperty-geometry:rect(361 2144 80 80);
	background:url(skin/picture/14/hook.png) 0 center no-repeat rgba(255,255,255,0);
}
QPushButton#btn_confirm:hover{
	background:url(skin/picture/14/hook_hover.png) 0 center no-repeat rgba(255,255,255,0);
}
QPushButton#btn_confirm:pressed{
	background:url(skin/picture/14/hook_pressed.png) 0 center no-repeat rgba(255,255,255,0);
}

